<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" text="Stored Elements" 
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="clusterPanel"
            fx:controller="edu.unibi.agbi.editor.presentation.controller.editor.panel.ClusterPanelController" >
   <content>
      <VBox spacing="10.0">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="buttonDisableClustered" mnemonicParsing="false" text="Disable" />
                  <Button fx:id="buttonEditClustered" mnemonicParsing="false" text="Edit" />
               </children>
            </HBox>
            <ListView fx:id="listClusteredElements" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </content>
</TitledPane>
