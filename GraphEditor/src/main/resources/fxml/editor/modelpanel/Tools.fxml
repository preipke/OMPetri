<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<TitledPane animated="false" text="Tools" 
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="edu.unibi.agbi.gnius.business.controller.editor.modelpanel.ToolsController">
    <content>
        <AnchorPane>
            <children>
                <GridPane alignment="TOP_CENTER" hgap="10.0" vgap="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <MenuButton fx:id="buttonCreate" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Create" textAlignment="RIGHT" GridPane.halignment="RIGHT">
                            <items>
                                <MenuItem fx:id="buttonCreatePlace" text="Place" />
                                <MenuItem fx:id="buttonCreateTransition" text="Transition" />
                            </items>
                        </MenuButton>
                        <Button fx:id="buttonRemove" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Remove" GridPane.columnIndex="1" GridPane.halignment="LEFT" />
                        <Button fx:id="buttonClusterCreate" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Cluster" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                        <Button fx:id="buttonClusterRemove" mnemonicParsing="false" prefHeight="35.0" prefWidth="70.0" text="Spread" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </GridPane>
            </children>
        </AnchorPane>
    </content>
</TitledPane>
