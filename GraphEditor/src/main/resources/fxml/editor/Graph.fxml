<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<TabPane tabClosingPolicy="ALL_TABS"
         xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
         fx:id="editorTabPane" fx:controller="edu.unibi.agbi.editor.presentation.controller.editor.GraphController">
    <tabs>
        <Tab closable="true">
            <content />
        </Tab>
        <Tab closable="false" disable="true">
            <content />
            <graphic>
                <Button fx:id="buttonCreateTab" mnemonicParsing="false" text="+" />
            </graphic>
        </Tab>
    </tabs>
</TabPane>
