<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<TabPane tabClosingPolicy="ALL_TABS" 
         xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
         fx:id="editorTabPane" fx:controller="edu.unibi.agbi.gnius.business.controller.editor.GraphEditorController">
    <tabs>
        <Tab closable="true">
            <content />
        </Tab>
        <Tab closable="false" disable="true">
            <content />
            <graphic>
                <Button fx:id="buttonCreateTab" mnemonicParsing="false" text="+" />
            </graphic>
        </Tab>
    </tabs>
</TabPane>
